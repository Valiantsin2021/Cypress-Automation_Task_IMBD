{
  "name": "Valentin_Lutchanka-AutomationTest",
  "version": "1.0.0",
  "description": "Valentin_Lutchanka-AutomationTest",
  "main": "index.js",
  "scripts": {
    "pretest": "rimraf ./cypress/results/* && rimraf ./mochawesome-report && rimraf ./index.json && rimraf ./allure-report && rimraf allure-results",
    "test": "npx cypress run -b chrome --record --key 115c3c0c-1f0f-409c-ae3a-3eec75c56c09 --env allure=true && npm run mocha:merge && npm run mocha:marge",
    "test:dd": "npx cypress run --env allure=true --spec cypress/e2e/data_driven/*.js",
    "test:ed": "npx cypress run --env allure=true --spec cypress/e2e/env_driven/*.js",
    "cy:open": "npx cypress open",
    "mocha:merge": "npx mochawesome-merge cypress/results/*.json > index.json",
    "mocha:marge": "npx marge index.json",
    "posttest": "allure generate allure-results --clean -o allure-report",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Valentin Lutchanka",
  "license": "ISC",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.34.0",
    "cypress": "^10.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2"
  }
}
